<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="bot">

    <xacro:arg name="is_ignition" default="true"/>

    <xacro:include filename="$(find isaac_ros_amr_navigation)/robot_description/urdf/bot_gazebo.xacro"/>
    <xacro:include filename="$(find isaac_ros_amr_navigation)/robot_description/urdf/amr_controller.xacro"/> 
    
    
    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>
    <material name="red">
        <color rgba="0.8 0 0 1"/>
    </material>
    <material name="green">
        <color rgba="0 0.8 0 1"/>
    </material>
    <material name="black">
        <color rgba="0.15 0.15 0.15 1"/>
    </material>
    <material name="white">
        <color rgba="0.9 0.9 0.9 1"/>
    </material>
    
    
    <link name="base_footprint"/>
    
    
    <link name="base_link">
        <visual>
            <origin xyz="0 0 0.015" rpy="0 0 0"/>
            <geometry>
                <box size="0.15 0.12 0.03"/>
            </geometry>
            <material name="blue"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0.015" rpy="0 0 0"/>
            <geometry>
                <box size="0.15 0.12 0.03"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0.015" rpy="0 0 0"/>
            <mass value="2.0"/>
            <inertia ixx="0.0067" ixy="0.0" ixz="0.0" 
                     iyy="0.0100" iyz="0.0" izz="0.0150"/>
        </inertial>
    </link>
    
    <joint name="basejoint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.033" rpy="0 0 0"/>
    </joint>

    
    <link name="right_wheel_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707963 0 0"/>
            <geometry>
                <cylinder radius="0.033" length="0.02"/>
            </geometry>
            <material name="black"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="1.5707963 0 0"/>
            <geometry>
                <cylinder radius="0.033" length="0.02"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="0.000139" ixy="0.0" ixz="0.0" 
                     iyy="0.000139" iyz="0.0" izz="0.000217"/>
        </inertial>
    </link>

    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel_link"/>
        <origin xyz="0 -0.07 0" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    
    <link name="left_wheel_link">
        <visual>
            <origin xyz="0 0 0" rpy="-1.5707963 0 0"/>
            <geometry>
                <cylinder radius="0.033" length="0.02"/>
            </geometry>
            <material name="black"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="-1.5707963 0 0"/>
            <geometry>
                <cylinder radius="0.033" length="0.02"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="0.000139" ixy="0.0" ixz="0.0" 
                     iyy="0.000139" iyz="0.0" izz="0.000217"/>
        </inertial>
    </link>

    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel_link"/>
        <origin xyz="0 0.07 0" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    
    <link name="caster_front_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.008"/>
            </geometry>
            <material name="white"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.008"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                     iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="caster_front_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_front_link"/>
        <origin xyz="0.05 0 -0.025" rpy="0 0 0"/>
    </joint>
    
    
    <link name="caster_rear_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.008"/>
            </geometry>
            <material name="white"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.008"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                     iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="caster_rear_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_rear_link"/>
        <origin xyz="-0.05 0 -0.025" rpy="0 0 0"/>
    </joint>
    
    
    <link name="imu_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.02 0.02 0.01"/>
            </geometry>
            <material name="red"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.02 0.02 0.01"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.02"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                     iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="imu_joint" type="fixed">
        <parent link="base_link"/>
        <child link="imu_link"/>
        <origin xyz="0 0 0.035" rpy="0 0 0"/>
    </joint>
    
    
    <link name="laser_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.03"/>
            </geometry>
            <material name="red"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.03"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                     iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="laser_joint" type="fixed">
        <parent link="base_link"/>
        <child link="laser_link"/>
        <origin xyz="0.05 0 0.05" rpy="0 0 0"/>
    </joint>


    
    <link name="camera_mount_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.02 0.10 0.015"/>
            </geometry>
            <material name="black"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.02 0.10 0.015"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.05"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                     iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="camera_mount_joint" type="fixed">
        <parent link="base_link"/>
        <child link="camera_mount_link"/>
        <origin xyz="0.065 0 0.07" rpy="0 0 0"/>
    </joint>

    
    <link name="camera_left_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.01 0.025 0.025"/>
            </geometry>
            <material name="black"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.01 0.025 0.025"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                     iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="camera_left_joint" type="fixed">
        <parent link="camera_mount_link"/>
        <child link="camera_left_link"/>
        <origin xyz="0 0.0375 0" rpy="0 0 0"/>
    </joint>

    
    <link name="camera_left_optical_frame"/>
    <joint name="camera_left_optical_joint" type="fixed">
        <parent link="camera_left_link"/>
        <child link="camera_left_optical_frame"/>
        <origin xyz="0 0 0" rpy="-1.5707963 0 -1.5707963"/>
    </joint>

    
    <link name="camera_right_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.01 0.025 0.025"/>
            </geometry>
            <material name="black"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.01 0.025 0.025"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                     iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="camera_right_joint" type="fixed">
        <parent link="camera_mount_link"/>
        <child link="camera_right_link"/>
        <origin xyz="0 -0.0375 0" rpy="0 0 0"/>
    </joint>

    
    <link name="camera_right_optical_frame"/>
    <joint name="camera_right_optical_joint" type="fixed">
        <parent link="camera_right_link"/>
        <child link="camera_right_optical_frame"/>
        <origin xyz="0 0 0" rpy="-1.5707963 0 -1.5707963"/>
    </joint>

    
    <link name="camera_center_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.01 0.025 0.025"/>
            </geometry>
            <material name="black"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.01 0.025 0.025"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" 
                     iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="camera_center_joint" type="fixed">
        <parent link="camera_mount_link"/>
        <child link="camera_center_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    
    <link name="camera_center_optical_frame"/>
    <joint name="camera_center_optical_joint" type="fixed">
        <parent link="camera_center_link"/>
        <child link="camera_center_optical_frame"/>
        <origin xyz="0 0 0" rpy="-1.5707963 0 -1.5707963"/>
    </joint>
    
</robot>

